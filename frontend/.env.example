# Frontend Environment Variables

# Backend API URL
VITE_API_URL=http://localhost:5000/api

# Socket Server URL
VITE_SOCKET_URL=http://localhost:5000

# Use mock data (set to 'true' to force mock data, 'false' for backend)
VITE_USE_MOCK=false

# Use mock socket (set to 'false' for real socket.io connection)
VITE_USE_MOCK_SOCKET=true
